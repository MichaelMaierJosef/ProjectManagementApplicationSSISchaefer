    <div id="currentProjects" class="mt-2 d-grid gap-3" ondrop="drop(event,'active')" ondragover="allowDrop(event)" ondragenter="dragEnter(event)" ondragleave="dragLeave(event)">
        <h3 style="z-index:-2;">Aktuelle Projekte</h3>
        @{

            foreach (Project project1 in activeProjects)
            {
                string staticBackdropid = "id" + @project1.id;
                string staticBackdropTarget = "#" + @staticBackdropid;
                DateTime currentD = DateTime.Now;
                DateTime startD = default;
                DateTime endD = default;


                foreach (UserStory story in ViewBag.UserStories)
                {
                    if (story.project_id == project1.id)
                    {
                        if (startD.Equals(default) || startD < story.startDate)
                        {
                            startD = story.startDate;
                        }
                        if (endD.Equals(default) || endD > story.endDate)
                        {
                            endD = story.endDate;
                        }
                    }
                }

                string estimatedTimePercent = "0";

                if (!startD.Equals(default))
                {
                    double rangeDaysAll = (endD - startD).TotalDays;
                    double rangeDaysEndToCurrent = (currentD - startD).TotalDays;
                    estimatedTimePercent = ("" + (rangeDaysEndToCurrent / rangeDaysAll * 100)).Replace(',', '.');
                }

                            <div class="projectItem rounded-8" 
                            id="@("projectItem"+project1.id)" 
                            style="background-color: #EFEDED; display:flex;" 
                            draggable="true" ondragstart="drag(event,@project1.id)">
