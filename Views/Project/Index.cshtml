
<script src="~/js/projectlist-script.js"></script>
<!--<script type="text/javascript" src="~/js/story.js"></script>-->
<script type="text/javascript" src="~/js/search.js"></script>
<meta charset="utf-8" />

<style>
    body {
        background: white;
    }

    .form-outline {
        min-height: auto;
        padding: .33em .75em;
        border: 0px;
        border-style: none;
        background: transparent;
        transition: all .2s linear;
    }

    .form-control {
        border: 0px;
        border-style: none;
    }
</style>


<div id="topNavigation" class="me-2 d-flex justify-content-end">
    <div class="input-group w-25 rounded-8" style="background-color: #EFEDED;">
        <span class="input-group-text border-0" id="search-addon">
            <i class="fas fa-search"></i>
        </span>
        <input type="search"
               class="form-control bg-transparent autocomplete-input"
               placeholder=""
               aria-describedby="search-addon"
               id="keyWordInput" oninput="filterPrjectlist()"
               style="border-radius: 20px" />
    </div>
    <form asp-action="CreateEdit">
        <button type="submit" class="btn btn-link text-black btn-floating ms-2">
            <i class="fas fa-plus fa-2x"></i>
        </button>
    </form>

</div>
@{
    List<Project> activeProjects = new List<Project>();
    List<Project> pastProjects = new List<Project>();
    List<Project> futureProjects = new List<Project>();

    foreach (Project project in ViewBag.Projects)
    {
        if (project.tense.Equals("active"))
        {
            activeProjects.Add(project);
        }
        else if (project.tense.Equals("future"))
        {
            futureProjects.Add(project);
        }
        else
        {
            pastProjects.Add(project);
        }
    }
}

<div id="currentProjekts" class="mt-2 d-grid gap-3">
    <h3>Aktuelle Projekte</h3>
    @{
        foreach (Project project1 in activeProjects)
        {
            string staticBackdropid = "id" + @project1.id;
            string staticBackdropTarget = "#" + @staticBackdropid;
            DateTime currentD = DateTime.Now;
            DateTime startD = default;
            DateTime endD = default;

            DateTime endDOverdue = default;


            foreach (UserStory story in ViewBag.UserStories)
            {
                if (story.project_id == project1.id)
                {
                    if (startD.Equals(default) || startD > story.startDate)
                    {
                        startD = story.startDate;
                    }
                    if (endD.Equals(default) || endD < story.endDate)
                    {
                        endD = story.endDate;
                    }
                    if (endDOverdue.Equals(default) || (endDOverdue > story.endDate && story.endDate < currentD))
                    {
                        endDOverdue = story.endDate;
                    }
                }
            }

            string estimatedTimePercent = "0";
            string overdueTimePercent = "0";

            if (!startD.Equals(default))
            {
                double rangeDaysAll = (endD - startD).TotalDays;
                double rangeDaysStartToCurrent = (currentD - startD).TotalDays;

                double rangeDaysStartToOverdue = (endDOverdue - startD).TotalDays;
                double overdueTime = rangeDaysStartToOverdue / rangeDaysAll * 100;

                overdueTimePercent = ("" + (overdueTime)).Replace(',', '.');

                if (rangeDaysStartToCurrent < rangeDaysAll)
                {
                    estimatedTimePercent = ("" + ((rangeDaysStartToCurrent / rangeDaysAll * 100) - overdueTime)).Replace(',', '.');
                }
            }



            <div class="projectItem rounded-8" style="background-color: #EFEDED; display:flex">
                <a class="projectlist d-flex flex-fill" asp-action="Index" asp-route-projectid="@project1.id" asp-route-projectName="@project1.projectName" asp-controller="UserStory">
                    <h4 class="m-3 ms-4 text-black align-self-start" style="max-width:20%; min-width:20%">@project1.projectName</h4>
                    <div class="progress mx-2 my-auto bg-white rounded-8 flex-fill" style="height:15%">
                        <div class="progress-bar bg-success"
                         role="progressbar"
                         style="width: 0%;"
                         aria-valuenow="0"
                         aria-valuemin="0"
                         aria-valuemax="100"></div>
                        <div class="progress-bar bg-danger"
                         role="progressbar"
                         style="width: @(overdueTimePercent+"%");"
                         aria-valuenow="0"
                         aria-valuemin="0"
                         aria-valuemax="100"></div>
                        <div class="progress-bar bg-warning"
                         role="progressbar"
                         style="width: @(estimatedTimePercent+"%");"
                         aria-valuenow="0"
                         aria-valuemin="0"
                         aria-valuemax="100"></div>
                    </div>

                </a>
                <form asp-action="CreateEdit" asp-route-id="@project1.id">
                    <button type="submit"
                        class="btn btn-link btn-lg h-100 ms-2 align-self-end"
                        data-mdb-ripple-color="dark"
                        style="color:#6750A4">
                        EDIT
                    </button>
                </form>

                <button type="button" data-mdb-toggle="modal"
                    data-mdb-target="@staticBackdropTarget"
                    class="btn btn-link btn-lg h-100 me-3 align-self-end"
                    data-mdb-ripple-color="dark"
                    style="color:red">
                    DELETE
                </button>
            </div>
            <div class="modal fade"
             id="@staticBackdropid"
             data-mdb-backdrop="static"
             data-mdb-keyboard="false"
             tabindex="-1"
             aria-labelledby="staticBackdropLabel"
             aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered modal-sm">
                    <div class="modal-content">
                        <div class="modal-header bg-primary text-white d-flex justify-content-center">
                            <h5 class="modal-title">Wirklich Löschen?</h5>
                        </div>
                        <div class="modal-body d-flex justify-content-center">
                            <i class="fas fa-trash-alt fa-3x text-primary"></i>
                        </div>
                        <div class="modal-footer align-content-between d-flex justify-content-center">
                            <form asp-action="DeleteProject" asp-route-id="@project1.id">
                                <button type="submit" class="btn btn-outline-black" data-mdb-dismiss="modal">Ja</button>
                            </form>
                            <button type="button" class="btn btn-dark" data-mdb-dismiss="modal">Abbrechen</button>
                        </div>
                    </div>
                </div>
            </div>
        }
    }
