@{
    List<Project> activeProjects = new List<Project>();
    List<Project> pastProjects = new List<Project>();
    List<Project> futureProjects = new List<Project>();

    foreach (Project project in ViewBag.Projects)
    {
        if (project.tense.Equals("active"))
        {
            activeProjects.Add(project);
        }
        else if (project.tense.Equals("future"))
        {
            futureProjects.Add(project);
        }
        else
        {
            pastProjects.Add(project);
        }
    }


}
<script>
    function dragEnter(ev) {
        document.getElementById(ev.target.id + "Dummy").classList.add("showDrop");
    @{
        foreach (Project project1 in activeProjects)
        {


        }
    }
    

    }
    function dragLeave(ev) {
        document.getElementById(ev.target.id + "Dummy").classList.remove("showDrop");
    }


    function allowDrop(ev) {
        ev.preventDefault();

    }

    function drag(ev, id) {
        ev.dataTransfer.setData("id", id);
        console.log(ev.target.id);
        ev.dataTransfer.setData("projectItem", ev.target.id);

    }

    function drop(ev, tense) {
        var id = ev.dataTransfer.getData("id");
        var divToMove = ev.dataTransfer.getData("projectItem");
        console.log(divToMove);

        var data = {
            str_projectId: id,
            tense: tense
        };

        $.ajax({
            type: "POST",
            url: "/project/ChangeTense",
            data: data,
            success: function(result) {
                window.location.href = "../";
            }
        });

        ev.preventDefault();

    }
</script>

